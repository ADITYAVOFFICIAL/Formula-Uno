{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/main.py"
    }
  ],
  "functions": {
    "main.py": {
      "runtime": "python3.9",
      "maxDuration": 10,
      "memory": 1024
    }
  },
  "env": {
    "LOG_LEVEL": "INFO",
    "CACHE_DIR": "/tmp/f1_cache",
    "PYTHONDONTWRITEBYTECODE": "1"
  }
}
